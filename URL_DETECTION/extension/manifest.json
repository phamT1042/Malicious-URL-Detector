{
    "name": "Malicious URL Detection",
    "description": "Detect whether website is malicious or not",
    "permissions": ["tabs"],
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html"
    },
    "content_security_policy": {
      "script-src": "self",
      "object-src": "self"
    }
  }
  